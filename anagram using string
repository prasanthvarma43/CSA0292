#include<stdio.h>
#include<string.h>
int main()
{
    char a[10]="save";
    char b[10]="vase";
    char temp;
    int i,j,len1,len2;
    len1=strlen(a);
    len2=strlen(b);
    if(len1!=len2)
    {
        printf("entered string is not anagram\n");
     return 0;
    }
    for(i=0;i<len1-1;i++)
    {
        for(j=i+1;j<len1;j++)
        {
            if(a[i]>b[j])
            {
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
            if (b[i]>b[j])
            {
                temp=b[i];
                b[i]=b[j];
                b[j]=temp;
            }
        }
    }
    for(i=0;i<len1;i++)
    {
        if(a[i]!=b[i])
        {
            printf("strings are not anagram\n");
            return 0;
        }
    }
    printf("given strings are anagram");
        return 0;
}
