#include <stdio.h>

int main() {
  int a[2][2]={10,50,150,200};
  int b[2][2]={0,0,0,0};
   int i,j;
  for(i=0;i<2;i++)
  {
      for(j=0;j<2;j++)
      {
         b[i][j]=a[i][j];
      }
  }
  for(i=0;i<2;i++)
  {
      for(j=0;j<2;j++)
      {
          printf("%d\t",b[i][j]);
      }
      printf("\n");
  }

    return 0;
}
