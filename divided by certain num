#include <stdio.h>
int main() 
{
  int n1,n2,n3,n4;
  printf("enter any four numbers=");
  scanf("%d%d%d%d",&n1,&n2,&n3,&n4);
  if(n1%4==0)
  printf("%d is divisible by 4=%d\n",n1);
   if(n2%4==0)
  printf("%d is divisible by 4=%d\n",n2);
   if(n3%4==0)
  printf("%d is divisible by 4=%d\n",n3);
   return 0;
}
output;
enter any four numbers=4 7 8 9
4 is divisible by 4=0
8 is divisible by 4=0
