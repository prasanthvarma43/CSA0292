#include <stdio.h>

int main() 
{
    int a[3][3]={2,12,24,26,28,20,10,15,25};
    int i,j,avg,sum=0;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d\t",a[i][j]);
        }
        printf("\n");
    }
   for(i=0;i<3;i++)
   {
       for(j=0;j<3;j++)
       {
           sum+=a[i][j];
       }
   }
   avg=sum/9;
   printf("sum of matrix=%d\n",sum);
   printf("avg of matrix=%d",avg);
    return 0;
}
